{
    "driver": {
        "@id": "driver/lips4",
        "name": "lips4",
        "url": "http://www.openprinting.org/download/japanese_printers/",
        "thirdpartysupplied": null,
        "execution": {
            "ghostscript": null,
            "prototype": "gs -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -dNOMEDIAATTRS -dNOINTERPOLATE -sDEVICE=lips4%A%Z -sOutputFile=- -"
