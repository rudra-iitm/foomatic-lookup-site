        "obsolete": {
            "@replace": "hplip"
        },
        "thirdpartysupplied": null,
        "execution": {
            "ghostscript": null,
            "prototype": "gs -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -dNOMEDIAATTRS -dNOINTERPOLATE -sDEVICE=cdj880%A%Z -sOutputFile=- -"
