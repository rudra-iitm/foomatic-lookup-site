{
    "driver": {
        "@id": "driver/npdl",
        "name": "npdl",
        "url": "http://www.openprinting.org/download/japanese_printers/",
        "thirdpartysupplied": null,
        "execution": {
            "ghostscript": null,
            "prototype": "gs -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -dNOMEDIAATTRS -dNOINTERPOLATE -sDEVICE=npdl%A%Z -sOutputFile=- -"
